syntax = "v1"

import "wxmanager/api/msg.api"

@server(
    prefix: v1
    group: wx
)
service manager {
    @doc "get version"
    @handler version
    get /version () returns (VersionResp)
}

@server(
    prefix: v1/wx
    group: wx
)
service wx {
    @doc "wechat message"
    @handler message
    post /message () returns ()
}